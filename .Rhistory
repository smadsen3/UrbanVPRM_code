tifdt_fun = function(raster,name){
dt = as.data.table(as.data.frame(raster, xy=T))
dt = cbind(1:ncell(raster), dt)
setnames(dt,c("Index","x","y",name))
setkey(dt,Index,x,y)
return(dt)
}
